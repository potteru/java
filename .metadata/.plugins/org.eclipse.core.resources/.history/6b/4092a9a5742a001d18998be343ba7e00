package in.arrays;

public class DuplicateElementsInArray {

	public static void main(String[] args) {

		int[] arr = new int[] {1,2,3,3,3,2,5,2,65,4,4,9,8,0,};
		
		for(int i = 0; i<arr.length;i++) {
			for(int j=i+1;j< arr.length;j++) {
				if(arr[i] == arr[j]) {
					System.out.print(arr[j]+",");
				}
			}
			
		}
	}

}
