package controlStatements;

public class SwitchStatements {

	public static void main(String[] args) {

		switch(32) {
		case 10 :
			System.out.println("Today is Monday");
			break;
		case 20:
			System.out.println("Today is");
			break;
		case 30:
			System.out.println("Today is");
			break;
		case 40:
			System.out.println("Today is");
			break;
		case 50:
			System.out.println("Today is");
			break;
		case 60 : 
			System.out.println("Today is");
			break;
		case 70:
			System.out.println("Today is");
			break;
		default : 
			System.out.println("In put not matched");
		}
	}

}
